{
  "name": "auth",
  "private": true,
  "license": "MIT",
  "scripts": {
    "lint": "eslint . && prettier --check . && cd webapp && pnpm run lint && cd ..",
    "fix": "eslint . --fix && prettier --write .",
    "prepare": "husky"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^7.1.0",
    "acorn": "^8.11.3",
    "eslint": "^8.38.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-sort-exports": "^0.9.1",
    "eslint-plugin-unused-imports": "^3.1.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,scss,yml,yaml,html}": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  }
}
