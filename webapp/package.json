{
  "name": "webapp",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "run-s build:*",
    "build:package": "cd ../package && pnpm build",
    "build:panda": "panda",
    "build:remix": "remix vite:build",
    "deploy": "wrangler pages deploy ./build/client",
    "dev": "run-p dev:*",
    "dev:panda": "panda --watch",
    "dev:vite": "remix vite:dev",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "fix": "eslint --cache --cache-location ./node_modules/.cache/eslint --fix .",
    "start": "wrangler pages dev ./build/client",
    "typecheck": "tsc",
    "test": "vitest --coverage --ui --dir test",
    "test:pubkey": "vitest run test/pubkey.test.ts",
    "typegen": "wrangler types",
    "prepare": "panda codegen",
    "generate:migrations": "drizzle-kit generate",
    "apply:migrations:local": "wrangler d1 migrations apply --local maximum-idp-db-for-preview",
    "apply:migrations:preview": "wrangler d1 migrations apply --remote maximum-idp-db-for-preview",
    "apply:migrations:prod": "wrangler d1 migrations apply --remote maximum-idp-db"
  },
  "dependencies": {
    "@octokit/auth-app": "^7.1.1",
    "@remix-run/cloudflare": "^2.13.1",
    "@remix-run/cloudflare-pages": "^2.13.1",
    "@remix-run/react": "^2.13.1",
    "@saitamau-maximum/auth": "workspace:^",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.35.3",
    "hono": "^4.6.7",
    "hono-remix-adapter": "^0.3.0",
    "isbot": "^5.1.17",
    "jose": "^5.9.3",
    "octokit": "^4.0.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240925.0",
    "@hono/vite-dev-server": "^0.16.0",
    "@pandacss/dev": "^0.47.0",
    "@remix-run/dev": "^2.13.1",
    "@types/react": "^18.2.67",
    "@types/react-dom": "^18.3.0",
    "@vitest/coverage-v8": "^2.1.1",
    "@vitest/ui": "^2.1.1",
    "drizzle-kit": "^0.26.2",
    "eslint": "^9.11.1",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-react": "^7.36.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "globals": "^15.9.0",
    "jsdom": "^25.0.1",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.6.2",
    "vite": "^5.4.10",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^2.1.1",
    "wrangler": "^3.78.10"
  },
  "engines": {
    "node": ">=20.11.0"
  }
}
